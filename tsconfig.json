{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "target": "esnext",
    "typeRoots": [
      "assets/types"
    ],
    "types": [
      "node"
    ]
  },
  "exclude": [
    ".bash_aliases",
    ".env",
    ".env.local",
    ".git",
    ".gitattributes",
    ".gitignore",
    ".idea",
    ".php-cs-fixer.dist.php",
    ".travis.yml",
    "README.md",
    "bin",
    "composer.json",
    "composer.lock",
    "config",
    "dev",
    "doc",
    "docker",
    "docker-compose.yaml",
    "fixtures",
    "lib",
    "migrations",
    "node_modules",
    "package.json",
    "phpstan.neon.dist",
    "public",
    "src",
    "symfony.lock",
    "templates",
    "tsconfig.json",
    "utils",
    "var",
    "vendor",
    "yarn.lock"
  ],
  "include": [
    ".eslintrc.js",
    "assets",
    "vite.config.js"
  ]
}
