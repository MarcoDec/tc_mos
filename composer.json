{
  "autoload": {
    "files": [
      "utils/api.php",
      "utils/functions.php"
    ],
    "psr-4": {
      "App\\": "src/",
      "App\\Doctrine\\DBAL\\": [
        "lib/doctrine/dbal/lib/Doctrine/DBAL/",
        "lib/doctrine/dbal/src/"
      ],
      "App\\Doctrine\\ORM\\": "lib/doctrine/orm/lib/Doctrine/ORM/",
      "App\\Symfony\\Bridge\\Doctrine\\": "lib/symfony/doctrine-bridge/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "dev/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "patches": {
      "doctrine/dbal": {
        "Doctrine\\DBAL\\Schema\\Comparator::doCompareSchemas to public": "./patches/doctrine/dbal/src/Schema/Comparator.patch",
        "Doctrine\\DBAL\\Schema\\MySQLSchemaManager::doCompareSchemas to App Comparator": "./patches/doctrine/dbal/src/Schema/MySQLSchemaManager.patch"
      },
      "symfony/validator": {
        "Symfony\\Component\\Validator\\Context\\ExecutionContext: $translator & $translationDomain to public": "./patches/symfony/validator/Context/ExecutionContext.patch"
      }
    },
    "symfony": {
      "allow-contrib": false,
      "require": "6.1.*"
    }
  },
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "^8.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "api-platform/core": "^2.6.8",
    "cweagans/composer-patches": "^1.7.2",
    "doctrine/annotations": "^1.13.3",
    "doctrine/doctrine-bundle": "^2.7.0",
    "doctrine/doctrine-migrations-bundle": "^3.2.2",
    "doctrine/orm": "^2.13.1",
    "dragonmantank/cron-expression": "^3.3.1",
    "illuminate/collections": "^9.27.0",
    "jawira/case-converter": "^3.5.1",
    "nelmio/cors-bundle": "^2.2.0",
    "pentatrion/vite-bundle": "^2.1.1",
    "ronanguilloux/isocodes": "^2.3.6",
    "symfony/asset": "6.1.*",
    "symfony/console": "6.1.*",
    "symfony/dotenv": "6.1.*",
    "symfony/expression-language": "6.1.*",
    "symfony/flex": "^2.2.3",
    "symfony/framework-bundle": "6.1.*",
    "symfony/http-client": "6.1.*",
    "symfony/intl": "6.1.*",
    "symfony/monolog-bundle": "^3.8.0",
    "symfony/property-info": "6.1.*",
    "symfony/proxy-manager-bridge": "6.1.*",
    "symfony/runtime": "6.1.*",
    "symfony/security-bundle": "6.1.*",
    "symfony/serializer": "6.1.*",
    "symfony/twig-bundle": "6.1.*",
    "symfony/validator": "6.1.*",
    "symfony/workflow": "6.1.*",
    "symfony/yaml": "6.1.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "nunomaduro/larastan": "^2.2.0",
    "orchestra/testbench": "^7.7.0",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^1.8.3",
    "phpstan/phpstan-doctrine": "^1.3.12",
    "phpstan/phpstan-symfony": "^1.2.13",
    "phpunit/phpunit": "^9.5.24",
    "rector/rector": "^0.14.0",
    "symfony/debug-bundle": "6.1.*",
    "symfony/phpunit-bridge": "^6.1.3",
    "symfony/stopwatch": "6.1.*",
    "symfony/web-profiler-bundle": "6.1.*"
  },
  "scripts": {
    "auto-scripts": {
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "type": "project"
}
