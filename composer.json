{
  "autoload": {
    "files": [
      "utils/api.php",
      "utils/functions.php"
    ],
    "psr-4": {
      "App\\": "src/",
      "App\\ApiPlatform\\Core\\": "lib/api-platform/core/src/",
      "App\\Doctrine\\DBAL\\": [
        "lib/doctrine/dbal/lib/Doctrine/DBAL/",
        "lib/doctrine/dbal/src/"
      ],
      "App\\Doctrine\\ORM\\": "lib/doctrine/orm/lib/Doctrine/ORM/",
      "App\\Symfony\\Bridge\\Doctrine\\": "lib/symfony/doctrine-bridge/",
      "App\\Symfony\\Component\\Validator\\": "lib/symfony/validator/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "dev/",
      "App\\Symfony\\Component\\Stopwatch\\": "lib/symfony/stopwatch/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "cweagans/composer-patches": true
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "composer-exit-on-failure": true,
    "patches": {
      "api-platform/core": {
        "Open class": "./patches/api-platform/core/src/Annotation/ApiProperty.patch",
        "ApiProperty inheritance in AnnotationPropertyMetadataFactory": "./patches/api-platform/core/src/Metadata/Property/Factory/AnnotationPropertyMetadataFactory.patch",
        "ApiProperty inheritance in AnnotationPropertyNameCollectionFactory": "./patches/api-platform/core/src/Metadata/Property/Factory/AnnotationPropertyNameCollectionFactory.patch",
        "Change RequestBody type to RequestBody|array": "./patches/api-platform/core/src/OpenApi/Model/Operation.patch"
      }
    },
    "symfony": {
      "allow-contrib": false,
      "require": "6.0.*"
    }
  },
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "^8.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "api-platform/core": "^2.6",
    "cweagans/composer-patches": "^1.7",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "dragonmantank/cron-expression": "^3.3",
    "haydenpierce/class-finder": "^0.4.3",
    "illuminate/collections": "^9.5.1",
    "jawira/case-converter": "^3.4",
    "nelmio/cors-bundle": "^2.2",
    "pentatrion/vite-bundle": "^1.2",
    "ronanguilloux/isocodes": "^2.3",
    "symfony/asset": "6.0.*",
    "symfony/console": "6.0.*",
    "symfony/dotenv": "6.0.*",
    "symfony/expression-language": "6.0.*",
    "symfony/flex": "^2.1",
    "symfony/framework-bundle": "6.0.*",
    "symfony/http-client": "6.0.*",
    "symfony/intl": "6.0.*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/property-info": "6.0.*",
    "symfony/proxy-manager-bridge": "6.0.*",
    "symfony/runtime": "6.0.*",
    "symfony/security-bundle": "6.0.*",
    "symfony/serializer": "6.0.*",
    "symfony/twig-bundle": "6.0.*",
    "symfony/validator": "6.0.*",
    "symfony/workflow": "6.0.*",
    "symfony/yaml": "6.0.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "friendsofphp/php-cs-fixer": "^3.8",
    "jetbrains/phpstorm-attributes": "^1.0",
    "nunomaduro/larastan": "^2.1",
    "orchestra/testbench": "^7.2",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.12.19",
    "symfony/debug-bundle": "6.0.*",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "6.0.*",
    "symfony/web-profiler-bundle": "6.0.*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "type": "project"
}
