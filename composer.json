{
  "autoload": {
    "files": [
      "utils/api.php",
      "utils/functions.php"
    ],
    "psr-4": {
      "App\\": "src/",
      "App\\Doctrine\\DBAL\\": "lib/doctrine/dbal/lib/Doctrine/DBAL/",
      "App\\Doctrine\\ORM\\": "lib/doctrine/orm/lib/Doctrine/ORM/",
      "App\\Symfony\\Bridge\\Doctrine\\": "lib/symfony/doctrine-bridge/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "dev/",
      "App\\Symfony\\Component\\Stopwatch\\": "lib/symfony/stopwatch/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  },
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "require": {
    "php": "8.0.*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^2.6",
    "composer/package-versions-deprecated": "1.11.99.5",
    "doctrine/annotations": "^1.13",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.11",
    "dragonmantank/cron-expression": "^3.3",
    "jawira/case-converter": "^3.4",
    "nelmio/cors-bundle": "^2.2",
    "pentatrion/vite-bundle": "^1.1",
    "phpdocumentor/reflection-docblock": "^5.3",
    "ronanguilloux/isocodes": "^2.3",
    "symfony/asset": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^2.1",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-client": "5.4.*",
    "symfony/intl": "5.4.*",
    "symfony/mime": "5.4.*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/string": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/workflow": "5.4.*",
    "symfony/yaml": "5.4.*",
    "tightenco/collect": "^8.81"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "friendsofphp/php-cs-fixer": "^3.5",
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-symfony": "^1.1",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.36",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "type": "project"
}
