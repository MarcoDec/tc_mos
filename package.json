{
  "dependencies": {
    "bootstrap": "^5.2.1",
    "pinia": "^2.0.22",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.5.1",
    "sass": "^1.54.9",
    "vite": "^3.1.2",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-symfony": "^0.6.0"
  },
  "license": "proprietary",
  "private": true,
  "scripts": {
    "build": "vite build && echo '*\n!.gitignore' > public/build/.gitignore",
    "dev": "sudo rm -r public/build/*; vite",
    "eslint": "NODE_ENV=production eslint -c .eslintrc.js .eslintrc.js vite.config.js ./assets/**/*.{js,vue}",
    "eslint-fix": "NODE_ENV=production eslint -c .eslintrc.js --fix .eslintrc.js vite.config.js ./assets/**/*.{js,vue}",
    "postinstall": "echo '*\n!.gitignore' > node_modules/.gitignore",
    "postuninstall": "echo '*\n!.gitignore' > node_modules/.gitignore"
  }
