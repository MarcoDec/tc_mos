  "license": "proprietary",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && echo '*\n!.gitignore' > public/build/.gitignore",
    "dev": "vite",
    "eslint": "vue-tsc --noEmit && NODE_ENV=production eslint -c .eslintrc.js assets",
    "eslint-fix": "vue-tsc --noEmit && NODE_ENV=production eslint -c .eslintrc.js --fix assets",
    "openapi": "php bin/console api:openapi:export -o openapi.json && npx openapi-typescript openapi.json --immutable-types --output assets/types/openapi.d.ts && rm openapi.json",
    "postinstall": "echo '*\n!.gitignore' > node_modules/.gitignore",
    "postuninstall": "echo '*\n!.gitignore' > node_modules/.gitignore"
  }
