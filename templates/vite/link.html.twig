{% if entries.css is defined %}
    {% for entry in entries.css %}
        <link href="/{{ out_dir }}/{{ entry }}" rel="stylesheet"/>
    {% endfor %}
{% endif %}

{% if entries.imports is defined %}
    {% for entry in entries.imports %}
        <link href="/{{ out_dir }}/{{ entry }}" rel="modulepreload"/>
    {% endfor %}
{% endif %}
