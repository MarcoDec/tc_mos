{
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "dev/",
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "phpstan/extension-installer": true,
      "cweagans/composer-patches": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "patches": {
      "api-platform/core": {
        "Unfinalize ApiPlatform\\Doctrine\\Orm\\Filter\\OrderFilter": "./patches/api-platform/core/src/Doctrine/Orm/Filter/OrderFilter.patch",
        "Unfinalize ApiPlatform\\Doctrine\\Orm\\Filter\\SearchFilter": "./patches/api-platform/core/src/Doctrine/Orm/Filter/SearchFilter.patch"
      },
      "doctrine/dbal": {
        "Doctrine\\DBAL\\Schema\\Comparator::doCompareSchemas to protected": "./patches/doctrine/dbal/src/Schema/Comparator.patch",
        "use App\\Doctrine\\Comparator": "./patches/doctrine/dbal/src/Schema/MySQLSchemaManager.patch"
      }
    },
    "symfony": {
      "allow-contrib": false,
      "require": "6.1.*"
    }
  },
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "require": {
    "php": ">=8.1.6",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "api-platform/core": "^3.0.3",
    "cweagans/composer-patches": "^1.7.2",
    "doctrine/annotations": "^1.13.3",
    "doctrine/doctrine-bundle": "^2.7.1",
    "doctrine/doctrine-migrations-bundle": "^3.2.2",
    "doctrine/orm": "^2.13.3",
    "nelmio/cors-bundle": "^2.2.0",
    "phpdocumentor/reflection-docblock": "^5.3.0",
    "phpstan/phpdoc-parser": "^1.13.0",
    "symfony/asset": "6.1.*",
    "symfony/console": "6.1.*",
    "symfony/dotenv": "6.1.*",
    "symfony/expression-language": "6.1.*",
    "symfony/flex": "^2.2.3",
    "symfony/framework-bundle": "6.1.*",
    "symfony/monolog-bundle": "^3.8.0",
    "symfony/property-access": "6.1.*",
    "symfony/property-info": "6.1.*",
    "symfony/proxy-manager-bridge": "6.1.*",
    "symfony/runtime": "6.1.*",
    "symfony/security-bundle": "6.1.*",
    "symfony/serializer": "6.1.*",
    "symfony/translation": "6.1.*",
    "symfony/twig-bundle": "6.1.*",
    "symfony/validator": "6.1.*",
    "symfony/yaml": "6.1.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13.0",
    "phpstan/extension-installer": "^1.2.0",
    "phpstan/phpstan": "^1.9.1",
    "phpstan/phpstan-doctrine": "^1.3.22",
    "phpstan/phpstan-symfony": "^1.2.16",
    "rector/rector": "^0.14.7",
    "symfony/debug-bundle": "6.1.*",
    "symfony/stopwatch": "6.1.*",
    "symfony/web-profiler-bundle": "6.1.*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "type": "project"
}
